    "rules": {
      "no-console": 0,
      "comma-dangle": 0
    }
  },
  "author": "Tomas Varga <tomas.varga.cz@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "sharp": "^0.16.2"
  }
}
